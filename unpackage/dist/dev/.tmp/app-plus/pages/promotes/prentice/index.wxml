<view><view class="tabbar" id="tabbar"><view data-event-opts="{{[['tap',[['clickCell',[1]]]]]}}" class="{{['barcell',(isSelect===1)?'border':'']}}" bindtap="__e">一级</view><view data-event-opts="{{[['tap',[['clickCell',[2]]]]]}}" class="{{['barcell',(isSelect===2)?'border':'']}}" bindtap="__e">二级</view></view><scroll-view class="content"><block wx:for="{{prentices}}" wx:for-item="prentice" wx:for-index="__i0__" wx:key="nickname"><view class="cell"><image class="photo" mode="aspectFill" src="{{prentice.img}}"></image><view class="info1"><label class="_span">{{prentice.nickname}}</label><label style="float:right;color:#FB7551;text-align:right;font-size:12px;" class="_span">{{"累计收入:"+prentice.income}}</label></view><view class="info2"><label style="color:#ED4040;width:20%;margin-right:20px;" class="_span">{{prentice.level}}</label><label style="margin-left:150npx;" class="_span">{{prentice.time}}</label><label style="float:right;" class="_span">{{"总人数:"+prentice.prenticecount}}</label></view></view></block></scroll-view></view>