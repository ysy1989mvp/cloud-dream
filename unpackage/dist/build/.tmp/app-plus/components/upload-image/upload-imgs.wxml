<view class="upload-image-view"><block wx:for="{{imageList}}" wx:for-item="image" wx:for-index="index" wx:key="index"><block><view class="image-view"><image src="{{image}}" data-src="{{image}}" data-event-opts="{{[['tap',[['previewImage',['$event']]]]]}}" bindtap="__e"></image><view class="del-btn" data-index="{{index}}" data-event-opts="{{[['tap',[['deleteImage',['$event']]]]]}}" bindtap="__e"><view class="baicha"></view></view></view></block></block><block wx:if="{{imageList.length<imageLength}}"><view data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" class="add-view" bindtap="__e"><view class="cross"><view class="transverse-line"></view><view class="vertical-line"></view></view></view></block><view class="info">{{"上传图片，不超过"+imageLength+"张。(非必填)"}}</view></view>