}
.marquee_box {
	display: block;
	position: relative;
	width: 100%;
	height: 30px;
	overflow: hidden;
}
.marquee_list {
	display: block;
	position: absolute;
	top: 0;
	left: 0;
}
.marquee_top {
	-webkit-transition: all 0.5s;
	-o-transition: all 0.5s;
	transition: all 0.5s;
	margin-top: -30px
}
.marquee_list .li {
	height: 30px;
	line-height: 30px;
	font-size: 14px;
	padding-left: 20px;
}


