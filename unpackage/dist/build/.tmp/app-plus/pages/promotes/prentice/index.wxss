
.tabbar{
	background: #FFFFFF;
	height: 120rpx;
	width: 100%;
}
.barcell {
	float: left;
	height: 100rpx;
	width: 30%;
	line-height: 100rpx;
	text-align: center;
	margin: 0 10%;
}
.border{
	border-bottom: #007AFF 5rpx groove;
}
.content{
	margin-top: 20rpx;
}
.cell{
	background-color: #FFFFFF;
	height: 180rpx;
}
.photo{
	width: 140rpx;
	height: 140rpx;
	border-radius: 50px;
	margin-top: 20rpx;
	margin-left: 20rpx;
	float: left;
}
.info1{
	width: calc(100vw - 200rpx);
	float: left;
	margin-left: 30rpx;
	margin-top: 20rpx;
	height: 80rpx;
	line-height: 80rpx;
}
.info2{
	width: calc(100vw - 200rpx);
	float: left;
	margin-left: 30rpx;
	font-size: 12px;
}

