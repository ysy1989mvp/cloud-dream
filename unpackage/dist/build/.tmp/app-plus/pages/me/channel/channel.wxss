/* 弹窗样式 */
.pop {
	width: 100%;
	height: 100vh;
	background: rgba(0, 0, 0, 0.5);
	position: fixed;
	top: 0;
	left: 0;
}
.popContent {
	width: 500rpx;
	margin: 0 auto;
	background-color: #FFFFFF;
	border-radius: 10rpx;
	position: absolute;
	bottom: 50%;
	left: 50%;
	-webkit-transform: translateX(-50%);
	    -ms-transform: translateX(-50%);
	        transform: translateX(-50%);
}
.popContent image {
	display: block;
	width: 100rpx;
	height: 100rpx;
	margin: 12rpx auto;
}
.popText {
	display: block;
	font-size: 24rpx;
	color: #434343;
	width: 100%;
	text-align: center;
	line-height: 200%;
}
.popButtons {
	width: 100%;
	border-bottom-left-radius: 10rpx;
	border-bottom-right-radius: 10rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	    -ms-flex-pack: start;
	        justify-content: flex-start;
}
.popCancel,
.popSure {
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	font-size: 28rpx;
	line-height: 75rpx;
	text-align: center;
	border: 1rpx solid #F5F5F5;
	width: 50%;
}
.popCancel {
	color: #999999;
	border-bottom-left-radius: 10rpx;
}
.popSure {
	color: #434343;
	border-bottom-right-radius: 10rpx;
}
