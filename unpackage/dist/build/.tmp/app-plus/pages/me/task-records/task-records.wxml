<view class="container"><weui-tabs class="theTab" vue-id="1" current="{{current}}" tabs="{{tabs}}" sliderLeft="{{sliderLeft}}" sliderOffset="{{sliderOffset}}" data-event-opts="{{[['^tabClick',[['tabClick']]]]}}" bind:tabClick="__e" bind:__l="__l"></weui-tabs><view class="content"><block wx:for="{{data}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toExamine',['$0'],[[['data','',index,'id']]]]]]]}}" class="line" bindtap="__e"><block wx:if="{{item.task_images.length>0}}"><image class="left" src="{{imgurl+item.task_images[0]}}"></image></block><block wx:if="{{item.task_images.length<1}}"><image class="left" src="{{defaultImgSrc}}"></image></block><view class="right"><view class="line_inner"><view class="inner_left"><text class="tex bigFont">{{item.title}}</text><block wx:if="{{item.status==1}}"><text class="tex redFont">已抢单</text></block><block wx:if="{{item.status==2}}"><text class="tex redFont">待审核</text></block><block wx:if="{{item.status==3}}"><text class="tex redFont">已审核</text></block><block wx:if="{{item.status==4}}"><text class="tex redFont">未通过</text></block></view><text class="tex redFont">{{"￥"+item.publish_price}}</text></view><view class="line_inner"><view class="inner_left"><text class="tex">{{"No:"+item.task_no}}</text></view><text class="tex">{{"剩余"+item.endTimeHour+"小时"}}</text><block wx:if="{{item.status==1}}"><text class="tex">未完成</text></block><block wx:if="{{item.status==2}}"><text class="tex">待审核</text></block><block wx:if="{{item.status==3}}"><text class="tex">已完成</text></block><block wx:if="{{item.status==4}}"><text class="tex">未通过</text></block></view></view></view></block></view></view>